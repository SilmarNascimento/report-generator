<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              name="top5_chart" pageWidth="140"
              pageHeight="600"
              columnWidth="140" leftMargin="0"
              rightMargin="0" topMargin="0"
              bottomMargin="0"
              xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
        http://jasperreports.sourceforge.net/xsd/jasperreport.xsd">

    <field name="label" class="java.lang.String"/>
    <field name="acertos" class="java.lang.Integer"/>
    <field name="total" class="java.lang.Integer"/>
    <field name="acertosWidthPx" class="java.lang.Integer"/>
    <field name="totalWidthPx" class="java.lang.Integer"/>

    <detail>
        <band height="32" splitType="Stretch">

            <line>
                <reportElement x="44" y="0" width="1" height="32" forecolor="#CCCCCC"/>
                <graphicElement>
                    <pen lineWidth="0.5"/>
                </graphicElement>
            </line>

            <rectangle>
                <reportElement x="45" y="4" width="10" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 10]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="4" width="20" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 20]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="4" width="30" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 30]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="4" width="40" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 40]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="4" width="50" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 50]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="4" width="60" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 60]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="4" width="70" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 70]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="4" width="80" height="10" backcolor="#3993B0" forecolor="#3993B0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{acertosWidthPx} == 80]]></printWhenExpression>
                </reportElement>
            </rectangle>

            <rectangle>
                <reportElement x="45" y="16" width="10" height="10" backcolor="#C0C0C0" forecolor="#C0C0C0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{totalWidthPx} == 10]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="16" width="20" height="10" backcolor="#C0C0C0" forecolor="#C0C0C0"
                               mode="Opaque">
                    <printWhenExpression><![CDATA[$F{totalWidthPx} == 20]]></printWhenExpression>
                </reportElement>
            </rectangle>
            <rectangle>
                <reportElement x="45" y="16" width="30" height="10" backcolor="#C0C0C0" forecolor="#C0C0C0"
