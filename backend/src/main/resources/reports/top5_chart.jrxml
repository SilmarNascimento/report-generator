                <textElement verticalAlignment="Middle">
                    <font fontName="RedHatDisplay" size="7" isBold="true"/>
                </textElement>
                <text><![CDATA[ACERTOS]]></text>
            </staticText>

            <rectangle>
                <reportElement x="52" y="7" width="8" height="8" backcolor="#C0C0C0" forecolor="#C0C0C0" mode="Opaque"/>
