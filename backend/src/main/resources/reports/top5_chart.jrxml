            <textField isStretchWithOverflow="true">
                <reportElement x="0" y="2" width="42" height="26" backcolor="#FFFFFF" forecolor="#000000"
                               mode="Opaque"/>
                <textElement textAlignment="Right" verticalAlignment="Middle">
                    <font fontName="RedHatDisplay" size="7" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{label}]]></textFieldExpression>
            </textField>

            <rectangle>
                <reportElement x="122" y="2" width="18" height="14" backcolor="#FFFFFF" forecolor="#FFFFFF"
                               mode="Opaque"/>
            </rectangle>
            <textField>
                <reportElement x="122" y="2" width="18" height="14" forecolor="#3993B0" backcolor="#FFFFFF"
                               mode="Opaque"/>
                <textElement verticalAlignment="Middle">
                    <font fontName="RedHatDisplay" size="8" isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{acertos}]]></textFieldExpression>
            </textField>

            <rectangle>
                <reportElement x="122" y="14" width="18" height="14" backcolor="#FFFFFF" forecolor="#FFFFFF"
                               mode="Opaque"/>
            </rectangle>
            <textField>
